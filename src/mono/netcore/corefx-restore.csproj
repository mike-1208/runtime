<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>netcoreapp30</TargetFramework>
    <DisableImplicitFrameworkReferences>true</DisableImplicitFrameworkReferences>
    <EnableDefaultItems>false</EnableDefaultItems>
    <GenerateRuntimeConfigurationFiles>true</GenerateRuntimeConfigurationFiles>
  </PropertyGroup>
  <PropertyGroup>
    <SystemCompositionVersions>$(MicrosoftPrivateCoreFxNETCoreAppVersion.Replace("4.6.0", "1.3.0"))</SystemCompositionVersions>
    <SystemNumericsTensorsVersion>$(MicrosoftPrivateCoreFxNETCoreAppVersion.Replace("4.6.0", "0.2.0"))</SystemNumericsTensorsVersion>
    <SystemDataSqlClientVersion>$(MicrosoftPrivateCoreFxNETCoreAppVersion.Replace("4.6.0", "4.7.0"))</SystemDataSqlClientVersion>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Private.CoreFx.NETCoreApp" Version="$(MicrosoftPrivateCoreFxNETCoreAppVersion)" />
    <PackageReference Include="System.CodeDom" Version="$(MicrosoftPrivateCoreFxNETCoreAppVersion)" />
    <PackageReference Include="System.ComponentModel.Composition" Version="$(MicrosoftPrivateCoreFxNETCoreAppVersion)" />
    <PackageReference Include="System.ComponentModel.Composition.Registration" Version="$(MicrosoftPrivateCoreFxNETCoreAppVersion)" />
    <PackageReference Include="System.Composition.AttributedModel" Version="$(SystemCompositionVersions)" />
    <PackageReference Include="System.Composition.Convention" Version="$(SystemCompositionVersions)" />
    <PackageReference Include="System.Composition.Hosting" Version="$(SystemCompositionVersions)" />
    <PackageReference Include="System.Composition.Runtime" Version="$(SystemCompositionVersions)" />
    <PackageReference Include="System.Composition.TypedParts" Version="$(SystemCompositionVersions)" />
    <PackageReference Include="System.Configuration.ConfigurationManager" Version="$(MicrosoftPrivateCoreFxNETCoreAppVersion)" />
    <PackageReference Include="System.Data.Odbc" Version="$(MicrosoftPrivateCoreFxNETCoreAppVersion)" />
    <PackageReference Include="System.Data.SqlClient" Version="$(SystemDataSqlClientVersion)" />
    <PackageReference Include="System.Drawing.Common" Version="$(MicrosoftPrivateCoreFxNETCoreAppVersion)" />
    <PackageReference Include="System.IO.Packaging" Version="$(MicrosoftPrivateCoreFxNETCoreAppVersion)" />
    <PackageReference Include="System.IO.Pipelines" Version="$(MicrosoftPrivateCoreFxNETCoreAppVersion)" />
    <PackageReference Include="System.IO.Ports" Version="$(MicrosoftPrivateCoreFxNETCoreAppVersion)" />
    <PackageReference Include="System.Json" Version="$(MicrosoftPrivateCoreFxNETCoreAppVersion)" />
    <PackageReference Include="System.Net.WebSockets.WebSocketProtocol" Version="$(MicrosoftPrivateCoreFxNETCoreAppVersion)" />
    <PackageReference Include="System.Numerics.Tensors" Version="$(SystemNumericsTensorsVersion)" />
    <PackageReference Include="System.Reflection.MetadataLoadContext" Version="$(MicrosoftPrivateCoreFxNETCoreAppVersion)" />
    <PackageReference Include="System.Security.Cryptography.Pkcs" Version="$(MicrosoftPrivateCoreFxNETCoreAppVersion)" />
    <PackageReference Include="System.Security.Cryptography.Xml" Version="$(MicrosoftPrivateCoreFxNETCoreAppVersion)" />
    <PackageReference Include="System.Security.Permissions" Version="$(MicrosoftPrivateCoreFxNETCoreAppVersion)" />
    <PackageReference Include="System.ServiceModel.Syndication" Version="$(MicrosoftPrivateCoreFxNETCoreAppVersion)" />
    <PackageReference Include="System.Text.Encoding.CodePages" Version="$(MicrosoftPrivateCoreFxNETCoreAppVersion)" />
    <PackageReference Include="System.Text.Encodings.Web" Version="$(MicrosoftPrivateCoreFxNETCoreAppVersion)" />
    <PackageReference Include="System.Threading.Channels" Version="$(MicrosoftPrivateCoreFxNETCoreAppVersion)" />
    <PackageReference Include="System.Utf8String.Experimental" Version="$(MicrosoftPrivateCoreFxNETCoreAppVersion)" />
  </ItemGroup>
</Project>
